"xvkbd -xsendevent -text "\A\[Left]""
	m:0x4 + c:67
	Control + F1

"xvkbd -xsendevent -text "\A\[Right]""
	m:0x4 + c:68
	Control + F2

"xvkbd -xsendevent -text "\Cr""
	m:0x4 + c:69
	Control + F3

"xvkbd -xsendevent -text "\[F11]""
	m:0x4 + c:70
	Control + F4

"urxvtc -e htop"
	m:0x4 + c:71
	Control + F5

"sh -c 'echo $((`cat /sys/devices/platform/backlight/backlight/backlight/brightness` - 1)) > /sys/devices/platform/backlight/backlight/backlight/brightness'"
	m:0x4 + c:72
	Control + F6

"sh -c 'echo $((`cat /sys/devices/platform/backlight/backlight/backlight/brightness` + 1)) > /sys/devices/platform/backlight/backlight/backlight/brightness'"
	m:0x4 + c:73
	Control + F7

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
	m:0x4 + c:74
	Control + F8

